{
  "name": "Preact",
  "version": "1.0",
  "description": "Discover places in any city",
  "permissions": [
    "tabs",
    "storage",
    "geolocation",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["src/background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/android-192.png"
  },
  "icons": {
    "16": "images/android-192.png",
    "32": "images/android-192.png",
    "48": "images/android-192.png",
    "128": "images/android-192.png"
  },
  "manifest_version": 2
}
